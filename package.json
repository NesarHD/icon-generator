{
  "name": "electron-create-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "icon-gen": "^2.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "semantic-ui-react": "^0.88.2"
  },
  "devDependencies": {
    "@craco/craco": "^5.6.4",
    "@semantic-ui-react/craco-less": "^1.2.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "electron": "^13.6.6",
    "semantic-ui-less": "^2.4.1",
    "wait-on": "^4.0.1"
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "electron-dev": "concurrently \"cross-env NODE_ENV=development PORT=3001 BROWSER=none npm start\" \"wait-on http://localhost:3001 && electron .\"",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder build -mw"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
